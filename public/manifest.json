{
  "name": "Memento Mori Calendar",
  "author": "Martin Wiu",
  "version": "1.0.0",
  "manifest_version": 3,
  "description": "Every time you open a new tab, a calendar shows how long you have left of your life.",
  "permissions": ["storage", "tabs"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icon-16x16.png",
    "32": "icon-32x32.png",
    "192": "icon-192x192.png",
    "512": "icon-512x512.png"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
